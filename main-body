full_Equation = input("put 2 number equation here: ")
split_equation = full_Equation.split()

firstnum = split_equation[0]
equation = split_equation[1]
secondnum = split_equation[2]

firstnum = int(firstnum)
secondnum = int(secondnum)

print(firstnum)
print(equation)
print(secondnum)
print("=")

if equation == "*" or "X" or "x":
   print(firstnum * secondnum)

if equation == "/":
   print(firstnum / secondnum)

if equation == "+":
   print(firstnum * secondnum)

if equation == "-":
   print(firstnum * secondnum)